<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/InTest"
            android:layout_width="@dimen/localsocket_button_width"
            android:layout_height="@dimen/localsocket_button_height"
            android:text="IN TEST"
            android:textSize="@dimen/textsize" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="aidl IN Show"
            android:textSize="@dimen/textsize" />
    </LinearLayout>

    <include layout="@layout/divider" />

    <TextView
        android:id="@+id/InBeforeInfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textsize" />

    <TextView
        android:id="@+id/InAfterInfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textsize" />

    <include layout="@layout/divider" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/OutTest"
            android:layout_width="@dimen/localsocket_button_width"
            android:layout_height="@dimen/localsocket_button_height"
            android:text="out test"
            android:textSize="@dimen/textsize" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/textsize"
            android:text="aidl  OUT Show"/>
    </LinearLayout>

    <include layout="@layout/divider" />

    <TextView
        android:id="@+id/OutBeforeInfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textsize" />

    <TextView
        android:id="@+id/OutAfterInfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textsize" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/InOutTest"
            android:layout_width="@dimen/localsocket_button_width"
            android:layout_height="@dimen/localsocket_button_height"
            android:text="Inout test"
            android:textSize="@dimen/textsize" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/textsize"
            android:text="aidl  INOUT Show"/>
    </LinearLayout>

    <include layout="@layout/divider" />

    <include layout="@layout/divider" />

    <TextView
        android:id="@+id/InOutBeforeInfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textsize" />

    <TextView
        android:id="@+id/InOutAfterInfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textsize" />

    <include layout="@layout/divider" />

</LinearLayout>